                                                                    user
                                                                    interface
                                                                    - to
                                                                    a
                                                                    location
                                                                    where
                                                                    more
                                                                    information
                                                                    can
                                                                    be
                                                                    obtained
                                                                    regarding
                                                                    the
                                                                    alert
                                                                </xsd:documentation>
                                                            </xsd:annotation>
                                                        </xsd:attribute>
                                                        <xsd:attribute
                                                            name="alertframe" type="report:cerifyUrl"
                                                            use="optional">
                                                            <xsd:annotation>
                                                                <xsd:documentation>
                                                                    A
                                                                    hyperlink
                                                                    that
                                                                    points
                                                                    to a
                                                                    thumbnail
                                                                    of
                                                                    the
                                                                    frame
                                                                    at
                                                                    the
                                                                    alert
                                                                    location
                                                                </xsd:documentation>
                                                            </xsd:annotation>
                                                        </xsd:attribute>
                                                        <xsd:attribute
                                                            name="alertid" type="xsd:integer" use="required">
                                                            <xsd:annotation>
                                                                <xsd:documentation>
                                                                    A
                                                                    unique
                                                                    identifier
                                                                    representing
                                                                    the
                                                                    alert
                                                                    that
                                                                    was
                                                                    raised
                                                                </xsd:documentation>
                                                            </xsd:annotation>
                                                        </xsd:attribute>
                                                        <xsd:attribute name="start"
                                                            type="xsd:string" use="optional">
                                                            <xsd:annotation>
                                                                <xsd:documentation>
                                                                    For quality alerts, this is
                                                                    the position at which this
                                                                    error condition began
                                                                </xsd:documentation>
                                                            </xsd:annotation>
                                                        </xsd:attribute>
                                                        <xsd:attribute name="end"
                                                            type="xsd:string" use="optional">
                                                            <xsd:annotation>
                                                                <xsd:documentation>
                                                                    For quality alerts, this is
                                                                    the position at which this
                                                                    error condition finished
                                                                </xsd:documentation>
                                                            </xsd:annotation>
                                                        </xsd:attribute>
                                                        <xsd:attribute name="channelname"
                                                            type="xsd:string" use="optional">
                                                            <xsd:annotation>
                                                                <xsd:documentation>
                                                                    For audio quality alerts,
                                                                    the name of the channel to
                                                                    which the alert applies, as
                                                                    given by the audio standard.
                                                                </xsd:documentation>
                                                            </xsd:annotation>
                                                        </xsd:attribute>
                                                        <xsd:attribute name="channelindex"
                                                            type="xsd:integer" use="optional">
                                                            <xsd:annotation>
                                                                <xsd:documentation>
                                                                     For audio quality alerts,
                                                                     the index of the channel to
                                                                     which the alert applies.
                                                                     Channel indices start from 1
                                                                </xsd:documentation>
                                                            </xsd:annotation>
                                                        </xsd:attribute>
                                                        <xsd:attribute name="trackId"
                                    						type="xsd:string" use="optional">
                                    						<xsd:annotation>
                                        						<xsd:documentation>
                                             						For audio alerts,
                                             						the track id in
                                             						which the alert applies.
                                        						</xsd:documentation>
                                    						</xsd:annotation>
                                						</xsd:attribute>
                                                    </xsd:complexType>
                                                </xsd:element>
                                            </xsd:sequence>
                                            <xsd:attribute name="name"
                                                type="xsd:string" use="required">
                                                <xsd:annotation>
                                                    <xsd:documentation>
                                                        Name of the
                                                        media file
                                                    </xsd:documentation>
                                                </xsd:annotation>
                                            </xsd:attribute>
                                            <xsd:attribute name="path"
                                                type="xsd:string" use="required">
                                                <xsd:annotation>
                                                    <xsd:documentation>
                                                        Location (URL)
                                                        from where the
                                                        processed media
                                                        file was
                                                        obtained
                                                    </xsd:documentation>
                                                </xsd:annotation>
                                            </xsd:attribute>
                                            <xsd:attribute
                                                name="progress" type="report:progress" use="required">
                                                <xsd:annotation>
                                                    <xsd:documentation>
                                                        Progress of
                                                        media file
                                                        processing
                                                        represented in
                                                        percentage
                                                    </xsd:documentation>
                                                </xsd:annotation>
                                            </xsd:attribute>
                                            <xsd:attribute name="result"
                                                type="report:resultType" use="required">
                                                <xsd:annotation>
                                                    <xsd:documentation>
                                                        The result for
                                                        the media file
                                                    </xsd:documentation>
                                                </xsd:annotation>
                                            </xsd:attribute>
                                            <xsd:attribute name="size"
                                                type="xsd:integer" use="required">
                                                <xsd:annotation>
                                                    <xsd:documentation>
                                                        Size of the
                                                        media file (in
                                                        bytes)
                                                    </xsd:documentation>
                                                </xsd:annotation>
                                            </xsd:attribute>
                                            <xsd:attribute
                                                name="started" type="xsd:dateTime" use="required">
                                                <xsd:annotation>
                                                    <xsd:documentation>
                                                        The time at
                                                        which the media
                                                        file started
                                                        processing
                                                    </xsd:documentation>
                                                </xsd:annotation>
                                            </xsd:attribute>
                                            <xsd:attribute name="status"
                                                type="report:fileStatusType" use="required">
                                                <xsd:annotation>
                                                    <xsd:documentation>
                                                        Processing
                                                        status for the
                                                        media file
                                                    </xsd:documentation>
                                                </xsd:annotation>
                                            </xsd:attribute>
                                            <xsd:attribute name="url"
                                                type="report:cerifyUrl" use="required">
                                                <xsd:annotation>
                                                    <xsd:documentation>
                                                        A hyperlink that
                                                        points back into
                                                        the Cerify Web
                                                        user interface -
                                                        to a location
                                                        where more
                                                        information can
                                                        be obtained
                                                        regarding the
                                                        media file
                                                    </xsd:documentation>
                                                </xsd:annotation>
                                            </xsd:attribute>
                                            <xsd:attribute
                                                name="posterframe" type="report:cerifyUrl" use="optional">
                                                <xsd:annotation>
                                                    <xsd:documentation>
                                                        A hyperlink that
                                                        points to a
                                                        thumbnail of the
                                                        first visually
                                                        distinct frame
                                                        of the media
                                                        file
                                                    </xsd:documentation>
                                                </xsd:annotation>
                                            </xsd:attribute>
                                            <xsd:attribute name="lastModified" type="xsd:dateTime" use="optional">
                        						<xsd:annotation>
                            						<xsd:documentation>
                                						last modified time (in long)
                            						</xsd:documentation>
                        						</xsd:annotation>
                    						</xsd:attribute>
                    						<xsd:attribute name="runNumber" type="xsd:long" use="optional">
                        						<xsd:annotation>
                            						<xsd:documentation>
                                						run number of job media file
                            						</xsd:documentation>
                        						</xsd:annotation>
                    						</xsd:attribute>
								<xsd:attribute name="owner" type="xsd:string" use="optional">
					                        <xsd:annotation>
					                        <xsd:documentation>
					                                Processed by this node
					                        </xsd:documentation>
						                </xsd:annotation>
					                </xsd:attribute>
                                        </xsd:complexType>
                                    </xsd:element>
                                </xsd:sequence>
                            </xsd:complexType>
                        </xsd:element>
                    </xsd:sequence>
                    <xsd:attribute name="completed" type="xsd:dateTime"
                        use="required">
                        <xsd:annotation>
                            <xsd:documentation>
                                The time at which the Job completed
                                processing
                            </xsd:documentation>
                        </xsd:annotation>
                    </xsd:attribute>
                    <xsd:attribute name="mediaset" type="xsd:string"
                        use="required">
                        <xsd:annotation>
                            <xsd:documentation>
                                The name of the MediaSet associated with
                                the Job
                            </xsd:documentation>
                        </xsd:annotation>
                    </xsd:attribute>
                    <xsd:attribute name="name" type="xsd:string"
                        use="required">
                        <xsd:annotation>
                            <xsd:documentation>
                                The name of the Job for which results
                                have been retrieved
                            </xsd:documentation>
                        </xsd:annotation>
                    </xsd:attribute>
                    <xsd:attribute name="priority"
                        type="report:priorityType" use="required">
                        <xsd:annotation>
                            <xsd:documentation>
                                The Job's priority
                            </xsd:documentation>
                        </xsd:annotation>
                    </xsd:attribute>
                    <xsd:attribute name="profile" type="xsd:string"
                        use="required">
                        <xsd:annotation>
                            <xsd:documentation>
                                The name of the Profile associated with
                                the Job
                            </xsd:documentation>
                        </xsd:annotation>
                    </xsd:attribute>
                    <xsd:attribute name="result"
                        type="report:resultType" use="required">
                        <xsd:annotation>
                            <xsd:documentation>
                                The result for the Job
                            </xsd:documentation>
                        </xsd:annotation>
                    </xsd:attribute>
                    <xsd:attribute name="started" type="xsd:dateTime"
                        use="required">
                        <xsd:annotation>
                            <xsd:documentation>
                                The time at which the Job started
                                processing
                            </xsd:documentation>
                        </xsd:annotation>
                    </xsd:attribute>
                    <xsd:attribute name="url" type="report:cerifyUrl"
                        use="required">
                        <xsd:annotation>
                            <xsd:documentation>
                                A hyperlink that points back into the
                                Cerify Web user interface - to a
                                location where more information can be
                                obtained regarding the Job
                            </xsd:documentation>
                        </xsd:annotation>
                    </xsd:attribute>
                </xsd:complexType>
            </xsd:element>

            <xsd:element name="GetJobResultsJobDoesntExistFault"
