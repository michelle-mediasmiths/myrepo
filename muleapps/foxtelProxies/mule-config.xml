    <description>
    Proxy connections for WFS solution
    </description>

	<pattern:http-proxy name="http-proxy-tcAdapter">
    	<http:inbound-endpoint address="http://0.0.0.0:8082/fx-TcAdapter" />
	    <http:outbound-endpoint address="http://localhost:8080/fx-TcAdapter" />
	</pattern:http-proxy>

	<pattern:http-proxy name="http-proxy-qcAdapter">
    	<http:inbound-endpoint address="http://0.0.0.0:8082/fx-qcAdapter" />
	    <http:outbound-endpoint address="http://localhost:8080/fx-qcAdapter" />
	</pattern:http-proxy>

	<pattern:http-proxy name="http-proxy-WFAdapter">
    	<http:inbound-endpoint address="http://0.0.0.0:8082/fx-WFAdapter" />
	    <http:outbound-endpoint address="http://localhost:8000/fx-WFAdapter" />
	</pattern:http-proxy>
	
	<pattern:http-proxy name="http-proxy-FSAdapter">
    	<http:inbound-endpoint address="http://0.0.0.0:8082/fx-FSAdapter" />
	    <http:outbound-endpoint address="http://localhost:8000/fx-FSAdapter" />
	</pattern:http-proxy>
	
	<pattern:http-proxy name="http-proxy-mayam-tasks-ws">
    	<http:inbound-endpoint address="http://0.0.0.0:8085/" />
	    <http:outbound-endpoint address="http://localhost:8084/" />
	</pattern:http-proxy>

	<pattern:http-proxy name="http-proxy-cerify">
    	<http:inbound-endpoint address="http://0.0.0.0:8086/" />
	    <http:outbound-endpoint address="http://192.168.2.13:80/" />
	</pattern:http-proxy>

	<pattern:http-proxy name="http-proxy-carbonwfs">
    	<http:inbound-endpoint address="http://0.0.0.0:8087/" />
	    <http:outbound-endpoint address="http://192.168.2.45:8731/" />
	</pattern:http-proxy>
	
	
	<pattern:http-proxy name="http-proxy-intalio">
    	<http:inbound-endpoint address="http://0.0.0.0:8083/ode" />
	    <http:outbound-endpoint address="http://localhost:8081/ode" />
	</pattern:http-proxy>

</mule>