<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<jaxws:bindings
		xmlns:wsdl="http://schemas.xmlsoap.org/wsdl/"
		xmlns:jaxws="http://java.sun.com/xml/ns/jaxws"
		xmlns:jaxb="http://java.sun.com/xml/ns/jaxb" jaxb:version="2.1"
		wsdlLocation="Rhozet.JobManager.JMServices.wsdl">

    <enableWrapperStyle>true</enableWrapperStyle>
    <enableAsyncMapping>false</enableAsyncMapping>
    <jaxws:bindings node="wsdl:definitions/wsdl:types/xs:schema/xs:complexType[@name='Module']/xs:sequence/xs:element[@name='Scheme']">
        <jaxb:class name="ModuleSchemeElement"/>
    </jaxws:bindings>   
    <jaxws:bindings node="wsdl:definitions/wsdl:types/xs:schema/xs:complexType[@name='Fetcher']/xs:sequence/xs:element[@name='Status']">
        <jaxb:class name="FetcherStatusElement"/>
    </jaxws:bindings>   
</jaxws:bindings>
