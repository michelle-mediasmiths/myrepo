            <xsd:complexType name="URLNotInMediaLocationFault">
                <xsd:complexContent>
                    <xsd:extension base="BaseCeritalkFault">
                        <xsd:sequence></xsd:sequence>
                    </xsd:extension>
                </xsd:complexContent>
            </xsd:complexType>

            <xsd:complexType name="JobNameInUseFault">
                <xsd:complexContent>
                    <xsd:extension base="BaseCeritalkFault">
                        <xsd:sequence></xsd:sequence>
                    </xsd:extension>
                </xsd:complexContent>
            </xsd:complexType>

            <xsd:complexType name="ProfileDoesntExistFault">
                <xsd:complexContent>
                    <xsd:extension base="BaseCeritalkFault">
                        <xsd:sequence></xsd:sequence>
                    </xsd:extension>
                </xsd:complexContent>
            </xsd:complexType>

            <xsd:complexType name="MediaSetNameInUseFault">
                <xsd:complexContent>
                    <xsd:extension base="BaseCeritalkFault">
                        <xsd:sequence></xsd:sequence>
                    </xsd:extension>
                </xsd:complexContent>
            </xsd:complexType>

            <xsd:complexType name="MediaFileNotInJobFault">
                <xsd:complexContent>
                    <xsd:extension base="BaseCeritalkFault">
                        <xsd:sequence></xsd:sequence>
                    </xsd:extension>
                </xsd:complexContent>
            </xsd:complexType>

        </xsd:schema>
    </types>

