            <!-- GetJobs request and response, and fault -->

            <xsd:element name="GetJobs">
                <xsd:complexType>
                    <xsd:sequence>
                        <xsd:element name="createTimeRangeFrom"
                            type="xsd:dateTime" minOccurs="0" nillable="true"
                            maxOccurs="1">
                            <xsd:annotation>
                                <xsd:documentation>
                                    If populated, then all active Jobs
                                    created on or after this date will
                                    be returned
                                </xsd:documentation>
                            </xsd:annotation>
                        </xsd:element>
                        <xsd:element name="createTimeRangeTo"
                            type="xsd:dateTime" minOccurs="0" nillable="true"
                            maxOccurs="1">
                            <xsd:annotation>
                                <xsd:documentation>
                                    If populated, then all active Jobs
                                    created on or before this date will
                                    be returned
                                </xsd:documentation>
                            </xsd:annotation>
                        </xsd:element>
                        <xsd:element name="status"
                            type="report:jobStatusType" maxOccurs="1" minOccurs="0"
                            nillable="true">
                            <xsd:annotation>
                                <xsd:documentation>
                                    If populated, then all active Jobs
                                    having this status will be returned
                                </xsd:documentation>
                            </xsd:annotation>
                        </xsd:element>
                    </xsd:sequence>
                </xsd:complexType>
            </xsd:element>

            <xsd:element name="GetJobsResponse">
                <xsd:complexType>
                    <xsd:sequence>
                        <xsd:element name="listOfJobs" type="xsd:string"
                            minOccurs="0" nillable="true" maxOccurs="unbounded">
                            <xsd:annotation>
                                <xsd:documentation>
                                    A list of active (non-archived) Job
                                    names meeting the criteria of the
                                    request
                                </xsd:documentation>
                            </xsd:annotation>
                        </xsd:element>
                    </xsd:sequence>
                </xsd:complexType>
            </xsd:element>

            <xsd:element name="GetJobsInvalidRangeFault"
                type="InvalidRangeFault">
            </xsd:element>

            <!-- GetSystemStatus request (empty) and response.  No faults -->

            <xsd:element name="GetSystemStatus">
                <xsd:complexType>
                    <xsd:sequence></xsd:sequence>
                </xsd:complexType>
            </xsd:element>

