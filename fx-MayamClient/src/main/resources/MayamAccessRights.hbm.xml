<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC
"-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
 	<class name="com.mediasmiths.mayam.accessrights.MayamAccessRights" table="MayamAccessRights">
 	 	<id column="id" name="id" type="java.lang.Long">
 	 	 	<generator class="org.hibernate.id.TableHiLoGenerator">
				<param name="table">idgen</param>
				<param name="column">NEXT</param>
 	 	 	</generator>
 	 	</id>
 	 	<property column="taskType" name="taskType" type="java.lang.String"/>
		<property column="taskState" name="taskState" type="java.lang.String"/>
		<property column="assetType" name="assetType" type="java.lang.String"/>
		<property column="groupName" name="groupName" type="java.lang.String"/>
		<property column="readAccess" name="readAccess" type="java.lang.Boolean"/>
		<property column="writeAccess" name="writeAccess" type="java.lang.Boolean"/>
		<property column="adminAccess" name="adminAccess" type="java.lang.Boolean"/>
 	</class>
</hibernate-mapping>