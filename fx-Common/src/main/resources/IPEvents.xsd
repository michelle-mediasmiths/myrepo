                        <xs:element name="completedInDateRange" type="xs:string" minOccurs="0"/>
                        <xs:element name="overdueInDateRange" type="xs:string" minOccurs="0"/>
                        <xs:element name="aggregatorID" type="xs:string" minOccurs="0"/>
                        <xs:element name="taskType" type="xs:string" minOccurs="0"/>
                        <xs:element name="completionDate" type="xs:dateTime" minOccurs="0"/>
                    </xs:sequence>
                </xs:extension>
            </xs:complexContent>
        </xs:complexType>
    </xs:element>

